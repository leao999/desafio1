fac = int(input())
x = fac
for i in range(1,x):
    fac *=i
fac

#se probo con 4 y resultado salio 24


#suma de dos numeros
print("Ingrese el primer valor entero")
a = int(input())
print("Ingrese el segundo valor entero")
b = int(input())
while b<a :
    print("Debe ingresar el segundo valor mayor al primero")
    print("Ingrese el segundo valor entero")
    b = int(input())

 #Inicia el calculo de la suma
b = b +1
suma = 0
for i in range(a,b):
    suma = suma + i
suma
print("El resultado es:", suma)

